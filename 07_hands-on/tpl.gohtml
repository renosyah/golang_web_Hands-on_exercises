<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<body>

    {{range .}}
        <h1>{{.Name}}</h1>
        <h2>Breakfast Menu</h2>
        {{range .Breakfast}}
            <p1>$ {{.Price}} - {{.Name}}</p>
        {{end}}

        <h1>Lunch Menu</h1>
        {{range .Lunch}}
                <p1>$ {{.Price}} - {{.Name}}</p>
        {{end}}

        <h1>Dinner Menu</h1>
        {{range .Dinner}}
                <p1>$ {{.Price}} - {{.Name}}</p>
        {{end}}

    {{end}}

</body>
</html>